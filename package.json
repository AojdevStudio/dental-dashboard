{
  "name": "dental-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "biome lint --files-ignore-unknown=true --write",
    "format": "biome format --write",
    "imports:fix": "biome check --fix --unsafe .",
    "typecheck": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:studio": "pnpm prisma studio",
    "prisma:seed": "pnpm dlx tsx prisma/seed.ts",
    "test:seed": "dotenv -e .env.test -- pnpm dlx tsx prisma/test-seed.ts",
    "postinstall": "prisma generate",
    "list": "node scripts/dev.js list",
    "generate": "node scripts/dev.js generate",
    "parse-prd": "node scripts/dev.js parse-prd",
    "test": "node scripts/utilities/test-runner.js run",
    "test:watch": "node scripts/utilities/test-runner.js watch",
    "test:coverage": "node scripts/utilities/test-runner.js coverage",
    "test:cloud": "echo \"Tests now run against cloud branch database - see .env.test for configuration\"",
    "migrate:uuid": "tsx scripts/database/data-migration/migrate-to-uuid.ts",
    "migrate:validate": "tsx scripts/database/data-migration/validate-migration.ts",
    "migrate:rollback": "tsx scripts/database/data-migration/rollback-uuid-migration.ts",
    "sync:post-reseed": "pnpm dlx tsx scripts/database/seeding/post-reseed-sync.ts",
    "sync:test-resilience": "pnpm dlx tsx scripts/testing/integration/test-sync-resilience.ts",
    "test:gas": "node scripts/testing/integration/test-google-apps-script-suite.cjs",
    "test:integration": "vitest --config vitest.integration.config.ts",
    "biome:check": "biome check .",
    "biome:fix": "biome check --write .",
    "biome:format": "biome format --write .",
    "biome:lint": "biome lint .",
    "prettier:yaml": "prettier --write \"**/*.{yml,yaml}\"",
    "pre-commit": "biome check --write . && pnpm test",
    "code-quality": "biome check . && pnpm test && pnpm build",
    "null-safety:check": "biome check --reporter=json 2>/dev/null | jq '.diagnostics[] | select(.rule.name == \"noNonNullAssertion\")' || echo 'No non-null assertions found'",
    "null-safety:report": "node scripts/debugging/null-safety-report.js",
    "security:scan": "node scripts/debugging/security-scan.js",
    "prepare": "husky",
    "gas:deploy": "node scripts/deployment/deploy-gas.cjs",
    "gas:deploy:all": "node scripts/deployment/deploy-gas.cjs --all",
    "gas:deploy:dentist": "node scripts/deployment/deploy-gas.cjs --type=dentist",
    "gas:deploy:hygienist": "node scripts/deployment/deploy-gas.cjs --type=hygienist",
    "gas:status": "node scripts/deployment/deploy-gas.cjs --status",
    "gas:help": "node scripts/deployment/deploy-gas.cjs --help",
    "gas:health": "node scripts/deployment/gas-health-check.cjs",
    "gas:health:all": "node scripts/deployment/gas-health-check.cjs --all",
    "gas:health:dentist": "node scripts/deployment/gas-health-check.cjs --type=dentist",
    "gas:health:hygienist": "node scripts/deployment/gas-health-check.cjs --type=hygienist",
    "ci:test": "./scripts/ci-local.sh",
    "fix:all": "pnpm biome:fix && pnpm prettier:yaml && pnpm typecheck",
    "biome:branch": "./scripts/biome-branch-check.sh",
    "test:e2e": "dotenv -e .env.test -- playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report"
  },
